<Window x:Class="DoubleDashPlotter.DependencyDownload"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DoubleDashPlotter"
        mc:Ignorable="d"
        Title="DependencyDownload" Height="201" Width="393">
    <Grid Background="#1C1C1C">
        <ProgressBar HorizontalAlignment="Center" Height="18" Margin="0,76,0,0" VerticalAlignment="Top" Width="346" ValueChanged="ProgressBar_ValueChanged" x:Name="ProgressBar_Download" Foreground="#FF00DEFB"/>
        <Label Content="LabelDownloadText" HorizontalAlignment="Center" Margin="0,45,0,0" VerticalAlignment="Top" Width="346" Foreground="White" x:Name="Label_Download"/>
        <ProgressBar HorizontalAlignment="Center" Height="18" Margin="0,125,0,0" VerticalAlignment="Top" Width="346" x:Name="ProgressBar_Setup" Foreground="#FF00DEFB"/>
        <Label Content="LabelSetupText" HorizontalAlignment="Center" Margin="0,99,0,0" VerticalAlignment="Top" Width="346" Foreground="White" x:Name="Label_Setup"/>
        <Label Content="Downloading Dependencies" VerticalAlignment="Top" Width="346" Foreground="White" FontSize="15" HorizontalAlignment="Center" Margin="0,10,0,0"/>
    </Grid>
</Window>
