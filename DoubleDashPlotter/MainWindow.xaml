<Window x:Class="DoubleDashPlotter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DoubleDashPlotter"
        mc:Ignorable="d"
        Title="MainWindow" Height="144" Width="460">
    <Grid Background="#1C1C1C">
        <Button Content="Browse" HorizontalAlignment="Left" Margin="10,45,0,0" VerticalAlignment="Top" Width="422" FontSize="15" Grid.ColumnSpan="2" Grid.RowSpan="2" Background="#007ACC" Foreground="White" Click="Button_Click" Name="Browse_Button"/>
        <Label Content=" Please provide a valid ROM file for Super Mario: Double Dash" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="White" FontSize="15" Width="422"/>
        <Button Content="I already have an instane running" HorizontalAlignment="Left" Margin="10,74,0,0" VerticalAlignment="Top" Width="422" FontSize="15" Background="#007ACC" Foreground="White" Click="Button_Click_Already_Running" x:Name="Already_Running_Button"/>

    </Grid>
</Window>
